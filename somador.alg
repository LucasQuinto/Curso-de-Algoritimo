algoritmo "soamdor"

var
n , s: inteiro
resp: caractere
inicio

      s<-0
      resp<- "s"
      
enquanto (resp = "s") faca
   escreva ("Digite o valor ==>")
   leia(n)
   s<- s+n
   escreva("voce quer continuar? ")
   leia(resp)
fimenquanto

   escreval("a soma dos valores é",s )

fimalgoritmo
