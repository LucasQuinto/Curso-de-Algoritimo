algoritmo "viagem"

var
din: real
inicio

escreva("quanto de dinheiro vc tem: ")
leia(din)

se (din >= 10.000)entao
   escreva("va a paris")
senao
 se(din < 10.000) e (din > 6.000) entao
   escreva("va a londres")

senao  (din < 6.000) entao
   escreva("nao vai viajar")
fimse
fimse

fimalgoritmo
